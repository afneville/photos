{
	"name": "photos",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "vitest run",
		"test:watch": "vitest --watch",
		"test:setup": "docker run -d --name dynamodb-local-test -p 8000:8000 amazon/dynamodb-local:latest -jar DynamoDBLocal.jar -inMemory -port 8000",
		"test:teardown": "docker stop dynamodb-local-test && docker rm dynamodb-local-test"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"@types/dockerode": "^3.3.43",
		"dockerode": "^4.0.7",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.873.0",
		"@aws-sdk/client-s3": "^3.879.0",
		"@aws-sdk/lib-dynamodb": "^3.873.0",
		"@aws-sdk/s3-request-presigner": "^3.879.0",
		"@radix-ui/colors": "^3.0.0",
		"@trpc/client": "^10.45.2",
		"@trpc/server": "^10.45.2",
		"@types/uuid": "^10.0.0",
		"fractional-indexing": "^3.2.0",
		"trpc-sveltekit": "^3.6.3",
		"uuid": "^11.1.0",
		"zod": "^4.1.3"
	}
}
